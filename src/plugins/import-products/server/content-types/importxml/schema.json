{
  "kind": "collectionType",
  "collectionName": "importxmls",
  "info": {
    "singularName": "importxml",
    "pluralName": "importxmls",
    "displayName": "Supplier"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "availability": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "shipping": {
      "type": "decimal",
      "default": 0,
      "min": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "useRetailPrice": {
      "type": "boolean",
      "default": false
    },
    "useRetailPriceBrands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "mappedBy": "suppliersWithRetailPrice"
    },
    "useRetailPriceCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "suppliersWithRetailPrice"
    },
    "useRetailPriceContainName": {
      "displayName": "useRetailPriceContainName",
      "type": "component",
      "repeatable": true,
      "component": "imports.use-retail-price"
    },
    "importedFile": {
      "type": "media",
      "required": false,
      "allowedTypes": [
        "files"
      ],
      "multiple": false
    },
    "importedURL": {
      "type": "text"
    },
    "minimumPrice": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "maximumPrice": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "lastRun": {
      "type": "datetime"
    },
    "report": {
      "type": "string"
    },
    "order_time": {
      "type": "time",
      "default": "11:50"
    },
    "isWhitelistSelected": {
      "type": "boolean",
      "default": true
    },
    "related_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "related_import"
    },
    "categories_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.categorymap",
      "mappedBy": "related_import"
    },
    "whitelist_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.whitelistmap",
      "mappedBy": "related_import"
    },
    "blacklist_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.blacklistmap",
      "mappedBy": "related_import"
    },
    "char_name_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.charnamemap",
      "mappedBy": "related_import"
    },
    "char_value_map": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::import-products.charvaluemap",
      "mappedBy": "related_import"
    },
    "stock_map": {
        "type": "relation",
        "relation": "oneToMany",
        "target": "plugin::import-products.stockmap",
        "mappedBy": "related_import"
    }
  }
}