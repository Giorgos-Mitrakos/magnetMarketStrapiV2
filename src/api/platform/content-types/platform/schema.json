{
  "kind": "collectionType",
  "collectionName": "platforms",
  "info": {
    "singularName": "platform",
    "pluralName": "platforms",
    "displayName": "Platform"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "entryURL": {
      "type": "string",
      "required": true
    },
    "merchantFeeCatalogue": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "export_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "export_to_platforms"
    },
    "order_time": {
      "type": "time",
      "default": "11:50"
    }
  }
}
