{
  "kind": "collectionType",
  "collectionName": "notify_mes",
  "info": {
    "singularName": "notify-me",
    "pluralName": "notify-mes",
    "displayName": "notify-me"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product",
      "inversedBy": "notify_me"
    },
    "productName": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "notified",
        "cancelled"
      ],
      "required": true,
      "default": "pending"
    },
    "notifiedAt": {
      "type": "datetime"
    }
  }
}
